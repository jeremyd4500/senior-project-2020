{
	"name": "senior-project-2020-frontend",
	"version": "1.0.0",
	"description": "The front end for the senior project",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config webpack.prod.js",
		"format": "prettier --write \"**/**.{js,jsx,json,css,less}\"",
		"jest": "jest --coverage",
		"start": "webpack-dev-server --hot --open --config webpack.dev.js"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/core": "^7.6.0",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/polyfill": "^7.8.3",
		"@babel/preset-env": "^7.6.0",
		"@babel/preset-react": "^7.0.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"@fortawesome/free-brands-svg-icons": "^5.12.0",
		"@fortawesome/free-solid-svg-icons": "^5.12.0",
		"@fortawesome/react-fontawesome": "^0.1.8",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"babel-preset-env": "^1.7.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"enzyme-to-json": "^3.4.4",
		"file-loader": "^4.2.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^4.2.3",
		"jest": "^25.1.0",
		"jsdom": "^16.1.0",
		"keymirror": "^0.1.1",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"nock": "^11.8.2",
		"prettier": "^1.18.2",
		"pretty-quick": "^2.0.1",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-axe": "^3.3.0",
		"react-date-picker": "^8.0.3",
		"react-dom": "^16.9.0",
		"react-focus-lock": "^2.4.1",
		"react-router": "3.2.4",
		"react-select": "^3.1.0",
		"style-loader": "^1.0.0",
		"terser-webpack-plugin": "^2.1.0",
		"url-loader": "^3.0.0",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.8",
		"webpack-dashboard": "^3.2.0",
		"webpack-dev-server": "^3.8.0",
		"webpack-merge": "^4.2.2"
	},
	"jest": {
		"verbose": false,
		"automock": false,
		"testURL": "http://localhost:8080/",
		"testEnvironment": "jsdom",
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"testMatch": [
			"**/*.test.js"
		],
		"setupFiles": [
			"./src/SetupJest.js"
		],
		"collectCoverage": true,
		"coverageReporters": [
			"text",
			"json",
			"html"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}",
			"!src/index.js",
			"!src/SetupJest.js",
			"!**/node_modules/**",
			"!**/build/**",
			"!**/.vscode/**"
		],
		"modulePaths": [
			"src"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
